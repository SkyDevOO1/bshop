// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id @default(cuid())
  displaName  String?
  email       String   @unique
  password    String
  phoneNumber String
  token       String
  creatAt     DateTime @default(now())
}
